include mixins
include variables
block variables




doctype html
html(lang="ru")
	head
		meta(charset="UTF-8")
		- title_page = typeof title ==="undefined" ? 'page_alomart' : 'page_alomart | '+title
		title #{title_page}
		meta(name="viewport", content="width=device-width initial-scale=1.0")
		//object(type='image/svg+xml', data='sprite.svg')
		//	| Your browser does not support SVG
		block assets
			link(rel="stylesheet", href="assets/css/main-min.css")

			script(src="assets/js/libs/jquery-3.5.1.min.js")
			script(src="assets/js/libs/svg-sprite.js")
			script(src="assets/js/libs/slick.min.js")
			script(src="assets/js/main-min.js")
			<link href="https://fonts.googleapis.com/css2?family=Montserrat:wght@300;400;500;700&display=swap" rel="stylesheet">

	body

		.main-wrapper

			include header
			//if(typeof titlePage !== "undefined" && titlePage)
			//	h1.all-page-title #{titlePage}

			main.main
				block content

		include footer.pug


























